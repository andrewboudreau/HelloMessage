{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ApprovalClient": {
    "BaseAddress": "http://localhost:7071/api/",

    "GetPending": "approve",
    "GetAudits": "approve/audits",

    "PostApproval": "approve/{submissionId}/by/{approverId}",
    "PostRejection": "reject/{submissionId}/by/{approverId}",
    "PostApprovalAll": "approve/all/by/{approverId}"
  },
  "AllowedHosts": "*"
}
